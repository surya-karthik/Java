public class Recursion {
    
  
    public static void main(String[] args) {
      
        int Fact_value= Fact (5);
        System.out.println(Fact_value);
        
       }
  private static int Fact(int n){
    if (n==1){
      return 1;
      
    }
    else{
      return n* Fact(n-1);
    }
  }
    
  
  
}
